{
  "presets": [
    "babel-preset-react",
    "es2015",
    [
      "react"
    ],
    [
      "env", {
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    "transform-react-jsx",
    "transform-class-properties",
    "transform-es2015-modules-commonjs",
    ["transform-runtime", {
      "regenerator": true
    }],
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    "syntax-dynamic-import"
  ],
  "ignore": [
    "*/node_modules,*/"
  ]
}